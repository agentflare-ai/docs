{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Agentflare",
  "description": "Protocol-level observability for AI agent tool servers. Track every decision, measure tool selection performance, and validate compliance in real-time.",
  "url": "https://docs.agentflare.com",
  "colors": {
    "primary": "#f97316",
    "light": "#f97316",
    "dark": "#09131a"
  },
  "favicon": "/favicon.ico",
  "openGraph": {
    "image": "/images/agentflare-logo.svg"
  },
  "anchors": [
    {
      "name": "Dashboard",
      "icon": "chart-line",
      "url": "https://app.agentflare.com"
    },
    {
      "name": "GitHub",
      "icon": "github",
      "url": "https://github.com/agentflare-ai"
    },
    {
      "name": "Early Access",
      "icon": "rocket",
      "url": "https://agentflare.com/early-access"
    }
  ],
  "navigation": {
    "groups": [
      {
        "group": "Getting Started",
        "icon": "play",
        "pages": [
          "content/introduction",
          "content/quickstart",
          "content/installation"
        ]
      },
      {
        "group": "Observability",
        "icon": "eye",
        "pages": [
          "content/features-overview",
          "content/thought-tracing",
          "content/memory-replay"
        ]
      },
      {
        "group": "Analytics & Monitoring",
        "icon": "chart-bar",
        "pages": [
          "content/tool-calls",
          "content/performance-analytics",
          "content/cost-tracking"
        ]
      },
      {
        "group": "Integration",
        "icon": "plug",
        "pages": [
          "content/mcp-proxy",
          "content/migration-guide"
        ]
      },
      {
        "group": "API Reference",
        "icon": "book-open",
        "pages": [
          "api-reference/introduction",
          "api-reference/endpoints/servers"
        ]
      },
      {
        "group": "Resources",
        "icon": "books",
        "pages": [
          "content/glossary",
          "content/best-practices"
        ]
      },
      {
        "group": "Support",
        "icon": "life-ring",
        "pages": [
          "content/troubleshooting"
        ]
      }
    ]
  },
  "logo": {
    "light": "/images/agentflare-logo.svg",
    "dark": "/images/agentflare-logo.svg",
    "href": "https://agentflare.com"
  },
  "background": {
    "color": {
      "light": "#ffffff",
      "dark": "#09131a"
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://app.agentflare.com"
      },
      {
        "label": "GitHub",
        "href": "https://github.com/agentflare-ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.agentflare.com/login"
    }
  },
  "search": {
    "prompt": "Search documentation..."
  },
  "footer": {
    "socials": {
      "github": "https://github.com/agentflare-ai",
      "x": "https://x.com/AgentflareAI"
    }
  },
  "redirects": [
    {
      "source": "/docs/getting-started",
      "destination": "/docs/introduction"
    }
  ],
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "seo": {
    "indexHiddenPages": false
  },
  "metadata": {
    "og:site_name": "Agentflare Documentation",
    "twitter:site": "@AgentflareAI"
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "css": "style.css"
}
